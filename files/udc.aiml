  </category>

  <category>
    <pattern>HELLO</pattern>
    <template><srai>HALO</srai></template>
  </category>

  <category>
    <pattern>HALLO</pattern>
    <template><srai>HALO</srai></template>
  </category>

  <category>
    <pattern>SELAMAT PAGI</pattern>
    <template><srai>HALO</srai></template>
  </category>

  <category>
    <pattern>SELAMAT SIANG</pattern>
    <template><srai>HALO</srai></template>
  </category>

  <category>
    <pattern>SELAMAT SORE</pattern>
    <template><srai>HALO</srai></template>
